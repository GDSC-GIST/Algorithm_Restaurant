import sys
N = int(sys.stdin.readline())
data=[]
count = 1 #맨 처음것은 포함
for _ in range(N):
    data.append(list(map(int, sys.stdin.readline().split())))
data.sort(key=lambda x: (x[1], x[0]))
endtime = data[0][1]
for i in range(1, N):
    if endtime <= data[i][0]:
        count += 1
        endtime = data[i][1]
print(count)
